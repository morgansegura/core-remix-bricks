{
  "name": "hfc",
  "private": true,
  "description": "A simple starter to get started quickly with React Bricks and Remix",
  "version": "1.3.7",
  "author": "",
  "license": "MIT",
  "scripts": {
    "build": "remix build",
    "codegen:bricks-ui": "ts-node scripts/codegen-bricks-ui.ts",
    "codegen:bricks-custom": "ts-node scripts/codegen-bricks-custom.ts",
    "codegen:bricks-layout": "ts-node scripts/codegen-bricks-layout.ts",
    "codegen:ui": "ts-node scripts/codegen-ui.ts",
    "dev": "remix dev",
    "format": "prettier --write .",
    "lint:css": "stylelint --cache --cache-location ./node_modules/.cache/stylelint \"app/**/*.css\"",
    "lint:js": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint": "run-p lint:*",
    "prepare": "husky install",
    "release": "ts-node scripts/release.ts",
    "start": "node -r dotenv/config node_modules/.bin/remix-serve build",
    "storybook:build": "storybook build",
    "storybook:chromatic": "chromatic --project-token=e18aacb73efa",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -b",
    "build-storybook": "storybook build"
  },
